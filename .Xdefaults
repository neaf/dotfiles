urxvt*saveLines:        32767
urxvt*scrollTtyKeypress:true
urxvt*scrollTtyOutput:  true
urxvt*scrollBar:        false
urxvt*loginShell:       true
urxvt*internalBorder:   0
urxvt*depth:             24
XTerm*termName: xterm-256color
urxvt*termName: rxvt-256color
!urxvt*inheritPixmap:    true
!urxvt*tintColor:        #212629
!urxvt*shading: 5
!urxvt*font:    xft:Terminus:pixelsize=12
urxvt*font:      -*-terminus-*-*-*-*-12-*-*-*-*-*-*-*
urxvt*boldFont:  -*-terminus-bold-*-*-*-14-*-*-*-*-*-*-*
urxvt*fading:             5
!urxvt*foreground:   #00FF00
!urxvt*background:   #222222
urxvt*cursorColor:  Orange
!urxvt*cursorColor2:   white
!urxvt*pointerColor:   white
urxvt*geometry:   90x55
Xcursor.theme: ComixCursors-Black-Small
Xscreensaver: spalsh 0
xterm*utf8:             1
xterm*eightBitInput:    true
xterm*metaSendsEscape:  true
Xft.antialias: 1
Xft.dpi: 93
!Xft.hinting: 1
!Xft.hintstyle: hintfull
!Xft.rgba: rgb

urxvt.perl-ext-common: default,matcher
urxvt.urlLauncher: firefox
urxvt.matcher.button: 2
urxvt.matcher.pattern.1: \\bwww\\.[\\w-]\\.[\\w./?&@#-]*[\\w/-]
urxvt.urgentOnBell: true

